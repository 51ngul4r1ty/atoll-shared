  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "transpile": "tsc",
    "build": "rollup -c",
    "prepublishOnly": "npm run clean && npm run build && npm run test:ci",
    "watch": "rollup -cw",
    "clean": "npx --no-install rimraf dist",
    "local-publish": "npx --no-install yalc publish",
    "local-publish-quick": "npm run build && npx --no-install yalc publish --force",
    "local-push": "npx --no-install yalc push",
    "sync:core": "cd .. && cd atoll-core && npx --no-install yalc add @atoll/shared && cd .. && cd atoll-shared",
    "sync": "npm run local-publish && npm run sync:core",
    "push": "npx --no-install yalc push",
    "test": "jest --watch --env=jsdom --coverage",
    "test:ci": "jest --env=jsdom --coverage",
    "test:update": "cross-env CI=true yarn-or-npm test --updateSnapshot",
    "test:report": "open-cli ./coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@csstools/normalize.css": "10.1.0",
