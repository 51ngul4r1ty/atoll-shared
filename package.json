{
  "name": "@atoll/shared",
  "version": "0.40.3",
  "author": {
    "name": "Kevin Berry",
    "email": "41717340+51ngul4r1ty@users.noreply.github.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.15.3",
    "npm": ">=6.10.1"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "transpile": "tsc",
    "build": "rollup -c",
    "prepublishOnly": "npm run clean && npm run build && npm run test:ci",
    "watch": "rollup -cw",
    "clean": "npx --no-install rimraf dist",
    "local-publish": "npx --no-install yalc publish",
    "local-publish-quick": "npm run build && npx --no-install yalc publish --force",
    "local-push": "npx --no-install yalc push",
    "sync:core": "cd .. && cd atoll-core && npx --no-install yalc add @atoll/shared && cd .. && cd atoll-shared",
    "sync": "npm run local-publish && npm run sync:core",
    "push": "npx --no-install yalc push",
    "test": "npm run test:tsc && npm run test:ci",
    "test:watch": "jest --watch --env=jsdom --coverage",
    "test:ci": "jest --env=jsdom --coverage",
    "test:tsc": "tsc --noEmit --project tsconfig.json",
    "test:update": "cross-env CI=true yarn-or-npm test --updateSnapshot",
    "test:report": "npx --no-install opener ./coverage/lcov-report/index.html",
    "prestorybook": "npm run build:lib && npm run build:storybook-html",
    "storybook": "npm run storybook-serve",
    "build:storybook": "npm run build:lib && build-storybook",
    "build:storybook-html": "npx --no-install ts-node  --project ./scripts/tsconfig.json ./scripts/build-storybook-html",
    "build:storybook-ts": "npx --no-install tsc -p tsconfig.storybook.json",
    "build:lib": "npm run build:storybook-ts && npm run copy-css-to-lib",
    "publish:storybook": "npm run build:storybook && npm run deploy:storybook",
    "deploy:storybook": "npx --no-install cpx \"./storybook-static/*\" \"./docs/storybook/\"",
    "copy-css-to-lib": "npx --no-install cpx \"./dist/index.cjs.css\" ./build/storybook",
    "storybook-serve": "start-storybook -p 6006",
    "gen:react-svg": "deno run --allow-read --allow-write --unstable ../atoll-scripts/build-svg-component/index.ts"
  },
  "dependencies": {
    "@csstools/normalize.css": "10.1.0",
    "@flopflip/react-broadcast": "10.1.11",
    "axios": "0.19.2",
    "connected-react-router": "6.8.0",
    "crypto": "1.0.1",
    "history": "4.10.1",
    "http-status-codes": "1.4.0",
    "i18next": "19.0.0",
    "immer": "5.3.6",
    "minimist": ">=1.2.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1",
    "react-i18next": "11.2.1",
    "react-markdown": "5.0.3",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "url-parse": "1.4.7",
    "websocket": "1.0.31"
  },
  "devDependencies": {
    "@flopflip/memory-adapter": "1.7.2",
    "@flopflip/types": "2.5.2",
    "@modular-css/rollup": "25.6.0",
    "@storybook/addon-actions": "6.1.6",
    "@storybook/addon-knobs": "6.1.15",
    "@storybook/addon-links": "6.1.6",
    "@storybook/addons": "6.1.6",
    "@storybook/cli": "6.1.6",
    "@storybook/react": "6.1.6",
    "@testing-library/react": "10.0.4",
    "@types/history": "4.7.5",
    "@types/i18next": "13.0.0",
    "@types/jest": "25.2.1",
    "@types/node": "13.13.4",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.5",
    "@types/react-helmet": "6.1.0",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.5",
    "@types/redux": "3.6.0",
    "@types/redux-mock-store": "1.0.2",
    "@types/url-parse": "1.4.3",
    "@types/websocket": "1.0.0",
    "acorn": ">=7.1.1",
    "autoprefixer": "9.7.6",
    "babel-loader": "8.1.0",
    "babel-preset-react-app": "9.1.2",
    "copy-webpack-plugin": "5.1.1",
    "cpx": "1.5.0",
    "cross-env": "7.0.2",
    "jest": "25.5.3",
    "jest-cli": "25.5.3",
    "jest-css-modules-transform": "4.0.0",
    "launchdarkly-js-client-sdk": "2.17.4",
    "opener": "1.5.2",
    "postcss-assets": "5.0.0",
    "postcss-custom-properties": "9.1.1",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-import": "12.0.1",
    "postcss-nested": "4.2.1",
    "redux-mock-store": "1.5.4",
    "rollup": "1.32.1",
    "rollup-plugin-image": "1.0.2",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-jsx": "1.0.3",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-postcss-modules": "2.0.1",
    "rollup-plugin-typescript2": "0.27.0",
    "storybook-addon-root-attribute": "1.0.0",
    "storybook-dark-mode": "1.0.3",
    "ts-jest": "25.4.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.2",
    "yalc": "1.0.0-pre.35"
  }
}
