    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "url-parse": "1.4.7",
    "websocket": "1.0.31"
  },
  "devDependencies": {
    "@flopflip/memory-adapter": "1.7.2",
    "@flopflip/types": "2.5.2",
    "@modular-css/rollup": "25.6.0",
    "@testing-library/react": "10.0.4",
    "@types/history": "4.7.5",
    "@types/i18next": "13.0.0",
